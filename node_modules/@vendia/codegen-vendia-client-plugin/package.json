  "devDependencies": {
    "@types/node": "^16.11.26",
    "auto-bind": "^5.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.4.2",
    "@graphql-codegen/visitor-plugin-common": "^2.7.4",
    "camelcase": "^6.3.0",
    "graphql": "15.8.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "watch": "watchlist src -- pnpm build",
    "watch:codegen-tools-generate-sdk": "watchlist dist ../codegen-tools/src -- ts-node-esm ../codegen-tools/test/integration-step-1.ts",
    "@comment": "Run 'pnpm dev' and review sdk output at codegen-tools/test/temp/generated.ts",
    "dev": "concurrently \"pnpm watch\" \"pnpm watch:codegen-tools-generate-sdk\""
  }
